{
  "name": "@multiverse/database",
  "version": "0.0.1",
  "scripts": {
    "test": "vitest",
    "typecheck": "tsc -noEmit",
    "build": "tsup src/index.ts --format cjs --minify --treeshake && pnpm zip",
    "zip": "tsx scripts/zip.ts"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.119",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/uuid": "^9.0.2",
    "tsconfig": "workspace:*",
    "tsx": "^3.12.7",
    "vitest": "^0.32.2"
  },
  "dependencies": {
    "aws-lambda": "^1.0.7",
    "uuid": "^9.0.0",
    "zod": "^3.21.4"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "target": "es6"
  }
}