{
  "name": "multiverse",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "sst bind next dev",
    "build": "turbo build",
    "build:orchestrator": "tsup packages/multiverse/src/Orchestrator/Orchestrator.ts --dts --format cjs --minify --clean --legacy-output --sourcemap --target es2020 --outDir packages/multiverse/src/Orchestrator/dist",
    "build:db": "tsup packages/multiverse/src/Database/Database.ts --dts --format cjs --minify --clean --sourcemap --target es2020 --outDir packages/multiverse/src/Database/dist",
    "deploy:db": "aws ecr get-login-password --region eu-central-1 | docker login --username AWS --password-stdin 529734186765.dkr.ecr.eu-central-1.amazonaws.com && DOCKER_BUILDKIT=1 docker build -t multiverse . && docker tag multiverse:latest 529734186765.dkr.ecr.eu-central-1.amazonaws.com/multiverse:latest && docker push 529734186765.dkr.ecr.eu-central-1.amazonaws.com/multiverse:latest",
    "run:db": "DOCKER_BUILDKIT=1 docker build -t multiverse . && docker run -p 9000:8080 multiverse",
    "start:docs": "cd apps/docs && pnpm start",
    "build:tsdocs": "typedoc --options packages/config/typedoc.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "turbo lint",
    "console": "turbo console",
    "typecheck": "tsc --noEmit",
    "deploy:dev": "sst deploy --stage=dev"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@vitest/coverage-v8": "^0.34.2",
    "aws-cdk-lib": "2.124.0",
    "constructs": "10.3.0",
    "eslint": "^8.41.0",
    "eslint-config-custom": "workspace:*",
    "jsdoc": "^4.0.2",
    "sst": "2.40.3",
    "tsconfig": "workspace:*",
    "tsup": "^7.1.0",
    "tsx": "^3.12.7",
    "turbo": "^1.10.7",
    "typedoc": "^0.25.10",
    "typedoc-plugin-inline-sources": "^1.0.2",
    "typedoc-plugin-markdown": "4.0.0-next.53",
    "typescript": "^5.1.3",
    "vitest": "^0.34.1"
  },
  "workspaces": [
    "packages/**/*",
    "apps/*"
  ],
  "dependencies": {
    "hnswlib-node": "^2.0.0"
  }
}