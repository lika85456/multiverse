{
  "name": "multiverse",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "sst bind next dev",
    "build": "turbo build",
    "build:orchestrator": "tsup packages/multiverse/src/Orchestrator/Orchestrator.ts --dts --format cjs --minify --clean --legacy-output --sourcemap --target es2020 --outDir packages/multiverse/src/Orchestrator/dist",
    "build:db": "tsup packages/multiverse/src/Database/Database.ts --dts --format cjs --minify --clean --sourcemap --target es2020 --outDir packages/multiverse/src/Database/dist",
    "deploy:db": "aws ecr get-login-password --region eu-central-1 | sudo docker login --username AWS --password-stdin 529734186765.dkr.ecr.eu-central-1.amazonaws.com && sudo docker build -t multiverse . && sudo docker tag multiverse:latest 529734186765.dkr.ecr.eu-central-1.amazonaws.com/multiverse:latest && sudo docker push 529734186765.dkr.ecr.eu-central-1.amazonaws.com/multiverse:latest",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "turbo lint",
    "console": "turbo console",
    "typecheck": "tsc --noEmit",
    "cdk:deploy": "sst deploy "
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@vitest/coverage-v8": "^0.34.2",
    "aws-cdk-lib": "2.91.0",
    "constructs": "10.2.69",
    "eslint": "^8.41.0",
    "eslint-config-custom": "workspace:*",
    "sst": "^2.24.21",
    "tsconfig": "workspace:*",
    "tsup": "^7.1.0",
    "tsx": "^3.12.7",
    "turbo": "^1.10.7",
    "typescript": "^5.1.3",
    "vitest": "^0.34.1"
  },
  "workspaces": [
    "packages/**/*",
    "apps/*"
  ],
  "dependencies": {
    "@multiverse/multiverse": "workspace:^",
    "hnswlib-node": "^2.0.0",
    "pnpm": "^8.6.12"
  }
}