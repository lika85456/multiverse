{
  "name": "multiverse",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "turbo lint",
    "console": "turbo console",
    "typecheck": "tsc --noEmit",
    "db:build": "docker build -t multiverse-database .",
    "db:run": "docker run -p 9000:8080 multiverse-database:latest",
    "db:deploy": "docker tag multiverse-database:latest 529734186765.dkr.ecr.eu-central-1.amazonaws.com/multiverse && docker push 529734186765.dkr.ecr.eu-central-1.amazonaws.com/multiverse"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@vitest/coverage-v8": "^0.34.2",
    "eslint": "^8.41.0",
    "eslint-config-custom": "workspace:*",
    "tsconfig": "workspace:*",
    "tsup": "^7.1.0",
    "tsx": "^3.12.7",
    "turbo": "^1.10.7",
    "typescript": "^5.1.3",
    "vitest": "^0.34.1"
  },
  "workspaces": [
    "packages/**/*",
    "apps/*"
  ],
  "dependencies": {
    "pnpm": "^8.6.12"
  }
}